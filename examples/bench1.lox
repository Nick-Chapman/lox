
fun fib(n) {
 if (n < 2) {
   return n;
 } else {
   return fib(n-1) + fib(n-2);
 }
}

var start = clock();

var i = 0;
var max = 30;
var sum = 0;

while (i <= max) {
  var res = fib(i);
  //print res;
  sum = sum + res;
  i = 1+i;
}

if (sum != 2178308) {
  print "unexpected result";
  print sum;
}

print clock() - start;

