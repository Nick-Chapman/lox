
example:
  #echo -n ~/code/lox/examples/pythagorean_triples.lox > $out
  echo -n ~/code/lox/examples/reassign-function-identifier.lox > $out

*five-ways: example @example src/lox.exe c/vm.exe
  echo "#J"; ~/.local/bin/jlox $$(cat example)
